{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm run install-all",
  "functions": {
    "server/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}